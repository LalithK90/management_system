    <attr sel="#petitionPriority" th:field="*{petitionPriority}"/>
    <attr sel="#petitionPriorityList" th:each="petitionPriority : ${petitionPriorities}" th:value="${petitionPriority}"
          th:text="${petitionPriority.petitionPriority}"/>
