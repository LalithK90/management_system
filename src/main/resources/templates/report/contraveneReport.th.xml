        <attr sel="#POU" th:text="${contraveneList[__${2}__].count}"/>
        &lt;!&ndash;UST&ndash;&gt;
        <attr sel="#UST" th:text="${contraveneList[__${3}__].count}"/>
        &lt;!&ndash;UPT&ndash;&gt;
